{
    "name": "tomatophp/filament-eddy",
    "type": "library",
    "description": "Eddy Server Manager & Tasks Runner for FilamentPHP",
    "keywords": [
        "php",
        "laravel",
        "eddy",
        "filament",
        "splade",
        "server-manager",
        "server",
        "manager"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "TomatoPHP\\FilamentEddy\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TomatoPHP\\FilamentEddy\\FilamentEddyServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Fady Mondy",
            "email": "info@3x1.io"
        }
    ],
    "require": {
        "php": "^8.1|^8.2",
        "tomatophp/console-helpers": "^1.1",
        "filament/filament": "^3.0.0"
    }
}
